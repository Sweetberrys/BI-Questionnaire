{"ast":null,"code":"export default {\n  name: \"Model\",\n  data() {\n    return {\n      pagesList: [],\n      tableData: [],\n      // 所有的数据\n      pageNum: 1,\n      // 当前的页码\n      pageSize: 4,\n      // 每页显示的个数\n      total: 0\n    };\n  },\n  mounted() {\n    this.load();\n  },\n  created() {\n    this.load(1);\n  },\n  // methods：本页面所有的点击事件或者其他函数定义区\n  methods: {\n    load(pageNum) {\n      this.$request.get('/pages/selectAll', {\n        params: {\n          open: '是'\n        }\n      }).then(res => {\n        this.pagesList = res.data || [];\n      });\n      if (pageNum) this.pageNum = pageNum;\n      this.$request.get('/pages/selectPage', {\n        params: {\n          pageNum: this.pageNum,\n          pageSize: this.pageSize,\n          name: this.name\n        }\n      }).then(res => {\n        this.tableData = res.data?.list;\n        this.total = res.data?.total;\n        this.totalPages = Math.ceil(this.total / this.pageSize); // 计算总页数\n\n        // 更新pagesList以展示当前页的内容\n        this.pagesList = this.tableData; // 或者使用.concat()方法等\n      });\n    },\n\n    reset() {\n      this.name = null;\n      this.load(1);\n    },\n    handleCurrentChange(pageNum) {\n      this.load(pageNum);\n    }\n  }\n};","map":{"version":3,"names":["name","data","pagesList","tableData","pageNum","pageSize","total","mounted","load","created","methods","$request","get","params","open","then","res","list","totalPages","Math","ceil","reset","handleCurrentChange"],"sources":["src/views/front/Model.vue"],"sourcesContent":["<template>\r\n    <div class=\"main-content\">\r\n        <div>\r\n            <el-row :gutter=\"10\">\r\n                <el-col :span=\"8\" v-for=\"item in pagesList\" :key=\"item.id\" style=\"margin-top:30px;margin-left: 160px\">\r\n                    <div class=\"card\" style=\"margin-bottom: 10px; \">\r\n                        <div style=\"display: flex; grid-gap: 10px; margin:20px 20px;\">\r\n                            <div style=\"flex: 1; width: 0\">\r\n                                <div style=\"margin-bottom: 10px; font-size: 30px\" class=\"line1\"><strong>{{ item.name }}</strong></div>\r\n                                <div style=\"color: #666;margin-bottom: 10px; font-size: 20px\" class=\"line1\"><strong>{{ item.descr }}</strong></div>\r\n                            </div>\r\n                            <div style=\"width: 110px;border-radius: 10px\">\r\n                                <img :src=\"item.img\" alt=\"\" style=\"width: 100%;border-radius: 10px\">\r\n                            </div>\r\n                        </div>\r\n\r\n                        <div>\r\n                            <el-button icon=\"el-icon-video-play\" type=\"warning\" size=\"mini\" style=\"font-size: 16px;background-color: rgb(25,80,134); border-color: rgb(25,80,134)\">点击预览</el-button>\r\n                            <el-button type=\"primary\" size=\"mini\" style=\"font-size: 16px;background-color: rgb(206,13,13); border-color: rgb(206,13,13)\">点击使用</el-button>\r\n                            <div style=\"color: #4f855a;margin-left:284px;margin-top: -25px; font-size: 16px\" class=\"line1\">{{ item.createTime }}</div>\r\n                        </div>\r\n                    </div>\r\n                </el-col>\r\n            </el-row>\r\n        </div>\r\n\r\n\r\n        <div class=\"pagination\" style=\"margin-left: 15%;color: white\" >\r\n            <el-pagination\r\n                background\r\n                @current-change=\"handleCurrentChange\"\r\n                :current-page=\"pageNum\"\r\n                :page-sizes=\"[5, 10, 20]\"\r\n                :page-size=\"pageSize\"\r\n                layout=\"total, prev, pager, next\"\r\n                :total=\"total\"\r\n                >\r\n            </el-pagination>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n\r\nexport default {\r\n    name:\"Model\",\r\n    data() {\r\n        return {\r\n            pagesList: [],\r\n            tableData: [],  // 所有的数据\r\n            pageNum: 1,   // 当前的页码\r\n            pageSize: 4,  // 每页显示的个数\r\n            total: 0,\r\n        }\r\n    },\r\n    mounted() {\r\n        this.load()\r\n    },\r\n    created() {\r\n        this.load(1)\r\n    },\r\n    // methods：本页面所有的点击事件或者其他函数定义区\r\n    methods: {\r\n        load(pageNum) {\r\n            this.$request.get('/pages/selectAll', {\r\n                params: { open: '是' }\r\n            }).then(res => {\r\n                this.pagesList = res.data || []\r\n            })\r\n\r\n            if (pageNum) this.pageNum = pageNum\r\n            this.$request.get('/pages/selectPage', {\r\n                params: {\r\n                    pageNum: this.pageNum,\r\n                    pageSize: this.pageSize,\r\n                    name: this.name,\r\n                }\r\n            }).then(res => {\r\n                this.tableData = res.data?.list\r\n                this.total = res.data?.total\r\n                this.totalPages = Math.ceil(this.total / this.pageSize) // 计算总页数\r\n\r\n                // 更新pagesList以展示当前页的内容\r\n                this.pagesList = this.tableData  // 或者使用.concat()方法等\r\n            })\r\n        },\r\n        reset() {\r\n            this.name = null\r\n            this.load(1)\r\n        },\r\n        handleCurrentChange(pageNum) {\r\n            this.load(pageNum)\r\n        },\r\n    }\r\n\r\n}\r\n</script>\r\n\r\n<style>\r\nbody{\r\n    /*background-image: url(\"../../assets/imgs/model_bg.jpg\");*/\r\n    /*background-size: cover;*/\r\n}\r\n.card {\r\n    padding: 20px;\r\n    background-color: #fff;\r\n    border-radius: 5px;\r\n    box-shadow: 0 2px 5px 0 rgba(0, 0, 0, 0.1);\r\n}\r\n.line1 {\r\n    overflow: hidden;\r\n    white-space: nowrap;\r\n    text-overflow: ellipsis;\r\n}\r\n</style>"],"mappings":"AA4CA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MACAC,SAAA;MAAA;MACAC,OAAA;MAAA;MACAC,QAAA;MAAA;MACAC,KAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA;EACA;EACAC,QAAA;IACA,KAAAD,IAAA;EACA;EACA;EAC