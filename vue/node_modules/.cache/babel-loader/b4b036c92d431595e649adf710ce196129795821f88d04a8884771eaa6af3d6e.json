{"ast":null,"code":"import * as echarts from 'echarts';\nexport default {\n  name: \"MyChart2.0\",\n  data() {\n    return {\n      pagesList: [],\n      tableData: [],\n      pageNum: 1,\n      pageSize: 2,\n      total: 0,\n      form: {},\n      backendData: '',\n      myChart: null,\n      // 添加一个变量来跟踪myChart是否已经准备好\n      myChartReady: false,\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}')\n    };\n  },\n  mounted() {\n    this.initMyChart(); // 首先初始化图表\n\n    this.load(this.pageNum); // 首先加载数据\n    this.$nextTick(() => {\n      this.initMyChart(); // 确保DOM更新后初始化图表\n    });\n  },\n\n  methods: {\n    formatTime(timestamp) {\n      const date = new Date(timestamp);\n      return `${date.getFullYear()}-${(date.getMonth() + 1).toString().padStart(2, '0')}-${date.getDate().toString().padStart(2, '0')} ${date.getHours().toString().padStart(2, '0')}:${date.getMinutes().toString().padStart(2, '0')}`;\n    },\n    load(pageNum) {\n      // ...加载数据的逻辑...\n      this.$nextTick(() => {\n        if (this.myChart && this.tableData[0]?.genChart) {\n          const genChart = JSON.parse(this.tableData[0].genChart);\n          this.myChart.setOption(genChart);\n        } else {\n          console.error(\"图表配置为空或不存在\");\n          this.$message.error(\"图表配置为空或不存在\");\n        }\n      });\n    },\n    handleCurrentChange(pageNum) {\n      this.load(pageNum);\n    },\n    initMyChart() {\n      let aiOptionElement = this.$refs.aiOption;\n      if (aiOptionElement && aiOptionElement.length > 0) {\n        this.myChart = echarts.init(aiOptionElement[0]);\n        this.myChartReady = true;\n      } else {\n        console.error('无法找到 aiOption 元素');\n        // 如果元素未找到，可以延迟重试\n        setTimeout(() => {\n          this.initMyChart();\n        }, 500); // 例如，500毫秒后再次尝试\n      }\n    }\n  }\n};","map":{"version":3,"names":["echarts","name","data","pagesList","tableData","pageNum","pageSize","total","form","backendData","myChart","myChartReady","user","JSON","parse","l