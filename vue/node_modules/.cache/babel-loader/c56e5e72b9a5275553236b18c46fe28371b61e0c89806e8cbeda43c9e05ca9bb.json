agesList: [],\r\n            tableData: [],\r\n            pageNum: 1,\r\n            pageSize: 2,\r\n            total: 0,\r\n            form: {},\r\n            backendData: '',\r\n            myChart: null,\r\n\r\n            user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\r\n        }\r\n    },\r\n    mounted() {\r\n        this.load(this.pageNum);\r\n        this.$nextTick(() => {\r\n            // 延迟加载图表配置\r\n            setTimeout(() => {\r\n                if (this.$refs.aiOption && this.$refs.aiOption[0]) {\r\n                    this.myChart = echarts.init(this.$refs.aiOption[0]);\r\n                    this.updateChart(); // 更新图表数据\r\n                } else {\r\n                    console.error('无法找到 aiOption 元素');\r\n                }\r\n            }, 1000); // 延迟时间可以根据实际情况调整\r\n        });\r\n    },\r\n    methods: {\r\n        updateChart() {\r\n            if (this.myChart && this.tableData[0]?.genChart) {\r\n                const genChart = JSON.parse(this.tableData[0].genChart);\r\n                this.myChart.setOption(genChart);\r\n            } else {\r\n                console.error(\"图表配置为空或不存在\");\r\n                this.$message.error(\"图表配置为空或不存在\");\r\n            }\r\n        },\r\n        formatTime(timestamp) {\r\n            const date = new Date(timestamp);\r\n            return `${date.getFullYear()}-${(date.getMonth() + 1).toString().padStart(2, '0')}-${date.getDate().toString().padStart(2, '0')} ${date.getHours().toString().padStart(2, '0')}:${date.getMinutes().toString().padStart(2, '0')}`;\r\n        },\r\n        load(pageNum) {\r\n            if (pageNum) this.pageNum = pageNum;\r\n            this.$request.get('/chart/selectPage', {\r\n                params: {\r\n                    pageNum: this.pageNum,\r\n                    pageSize: this.pageSize,\r\n                }\r\n            }).then(res => {\r\n                this.tableData = res.data?.list;\r\n                this.total = res.data?.total;\r\n                this.pagesList = this.tableData;\r\n                this.$nextTick(() => { // 确保DOM更新后再尝试设置图表选项\r\n                    if (this.myChart) { // 检查myChart是否已初始化\r\n                        if (this.tableData[0]?.genChart) {\r\n                            const genChart = JSON.parse(this.tableData[0].genChart);\r\n                            this.myChart.setOption(genChart);\r\n                        } else {\r\n                            console.error(\"genChart 为空或不存在\");\r\n                            this.$message.error(\"图表配置为空或不存在\");\r\n                        }\r\n                    } else {\r\n                        console.error(\"myChart 未初始化\");\r\n                    }\r\n                });\r\n            });\r\n\r\n            this.$nextTick(() => {\r\n                this.updateChart(); // 数据加载完成后更新图表\r\n            });\r\n        },\r\n        handleCurrentChange(pageNum) {\r\n            this.load(pageNum)\r\n        },\r\n    },\r\n};\r\n</script>\r\n\r\n<style>\r\nbody {\r\n    background-color: #dac9c9;\r\n}\r\n</style>\r\n"],"mappings":"AA0DA,YAAAA,OAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;MACAC,SAAA;MACAC,OAAA;MACAC,QAAA;MACAC,KAAA;MACAC,IAAA;MACAC,WAAA;MACAC,OAAA;MAEAC,IAAA,EAAAC,IAAA,CAAAC,KAAA,CAAAC,YAAA,CAAAC,OAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA,MAAAZ,OAAA;IACA,KAAAa,SAAA;MACA;MACAC,UAAA;QACA,SAAAC,KAAA,CAAAC,QAAA,SAAAD,KAAA,CAAAC,QAAA;UACA,KAAAX,OAAA,GAAAV,OAAA,CAAAsB,IAAA,MAAAF,KAAA,CAAAC,QAAA;UACA,KAAAE,WAAA;QACA;UACAC,OAAA,CAAAC,KAAA;QACA;MACA;IACA;EACA;;EACAC,OAAA;IACAH,YAAA;MACA,SAAAb,OAAA,SAAAN,SAAA,KAAAuB,QAAA;QACA,MAAAA,QAAA,GAAAf,IAAA,CAAAC,KAAA,MAAAT,SAAA,IAAAuB,QAAA;QACA,KAAAjB,OAAA,CAAAkB,SAAA,CAAAD,QAAA;MACA;QACAH,OAAA,CAAAC,KAAA;QACA,KAAAI,QAAA,CAAAJ,KAAA;MACA;IACA;IACAK,WAAAC,SAAA;MACA,MAAAC,IAAA,OAAAC,IAAA,CAAAF,SAAA;MACA,UAAAC,IAAA,CAAAE,WAAA,OAAAF,IAAA,CAAAG,QAAA,QAAAC,QAAA,GAAAC,QAAA,YAAAL,IAAA,CAAAM,OAAA,GAAAF,QAAA,GAAAC,QAAA,YAAAL,IAAA,CAAAO,QAAA,GAAAH,QAAA,GAAAC,QAAA,YAAAL,IAAA,CAAAQ,UAAA,GAAAJ,QA